<?xml version="1.0" encoding="UTF-8"?>
<MatchingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <matchingRules>
        <fullName>MatchByNameOrPhone</fullName>
        <booleanFilter>(1 AND 2) OR 3</booleanFilter>
        <label>Match By Name OR Phone</label>
