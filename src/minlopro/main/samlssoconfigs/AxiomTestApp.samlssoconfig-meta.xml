<?xml version="1.0" encoding="UTF-8"?>
<SamlSsoConfig xmlns="http://soap.sforce.com/2006/04/metadata">
    <identityLocation>SubjectNameId</identityLocation>
    <identityMapping>FederationId</identityMapping>
    <issuer>https://axiomsso.herokuapp.com</issuer>
    <name>Axiom Test App</name>
    <oauthTokenEndpoint>${SF_INSTANCE_URL}/services/oauth2/token</oauthTokenEndpoint>
    <redirectBinding>true</redirectBinding>
    <requestSignatureMethod>0</requestSignatureMethod>
    <requestSigningCertId>${SF_MINLOPRO_CERT_ID}</requestSigningCertId>
    <salesforceLoginUrl>${SF_INSTANCE_URL}</salesforceLoginUrl>
    <samlEntityId>${SF_INSTANCE_URL}</samlEntityId>
    <samlVersion>SAML2_0</samlVersion>
    <useConfigRequestMethod>true</useConfigRequestMethod>
    <useSameDigestAlgoForSigning>true</useSameDigestAlgoForSigning>
    <userProvisioning>false</userProvisioning>
    <!--Base64 encoded Axiom-IdpCert.cer-->
    <validationCert></validationCert>
</SamlSsoConfig>
