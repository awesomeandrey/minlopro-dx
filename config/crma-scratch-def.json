{
    "country": "US",
    "description": "CRM Analytics Scratch Org",
    "edition": "Developer",
    "features": [
        "API",
        "AuthorApex",
        "DebugApex",
        "DefaultWorkflowUser",
        "EnableSetPasswordInApi",
        "EinsteinAnalyticsPlus",
        "MultiCurrency",
        "StateAndCountryPicklist",
        "StreamingAPI"
