{
    "country": "US",
    "description": "Minlopro DX Scratch Org",
    "edition": "Developer",
    "features": [
        "API",
        "AuthorApex",
        "Communities",
        "ContactsToMultipleAccounts",
        "DebugApex",
        "DefaultWorkflowUser",
        "EnableSetPasswordInApi",
        "FieldAuditTrail",
        "ForceComPlatform",
        "Knowledge",
        "LightningServiceConsole",
        "LiveAgent",
        "LiveMessage",
        "PersonAccounts",
        "SalesforceContentUser",
        "SecurityEventEnabled",
        "ServiceCloud",
        "StateAndCountryPicklist",
        "StreamingAPI"
    ],
    "hasSampleData": false,
    "language": "en_US",
    "orgName": "minlopro-dx",
    "settings": {
        "apexSettings": {
            "enableCompileOnDeploy": false,
            "enableApexApprovalLockUnlock": true,
            "enableDisableParallelApexTesting": true
